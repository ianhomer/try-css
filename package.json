{
  "name": "try-css",
  "version": "1.0.0",
  "description": "Try CSS",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "dev": "parcel 'src/*.html' --lazy",
    "lint": "pnpm prettier",
    "lint:fix": "pnpm package:fix && pnpm prettier:fix",
    "package:fix": "format-package -w",
    "prepare": "husky install",
    "prettier": "pnpx prettier --check .",
    "prettier:fix": "pnpx prettier --write ."
  },
  "devDependencies": {
    "format-package": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1"
  }
}
